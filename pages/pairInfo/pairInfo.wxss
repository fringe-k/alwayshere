/* pages/pairInfo/pairInfo.wxss */
.pair-portraits{
  margin-top: 50rpx;
  margin-bottom: 100rpx;
  width:100%;
  height:30%;
  display:flex;
  justify-content: space-around;
  align-items: center;
}
.portrait{
  height: 200rpx;
  display:flex;
  justify-content: space-around;
  align-items: center;
}
.portrait.left{
  margin-left: 150rpx;
}
.portrait.right{
  margin-right: 150rpx;
}
.portrait-image{
  height: 160rpx;
  width: 160rpx;
  border: 0 solid #ffffff;
  border-radius: 250rpx;
  font-size:0;
}
.share-button{
  padding: 0;
}
.share-button::after{ 
  border:none;
}
.share-button image{
  height: 160rpx;
  width: 160rpx;
}
.item-line{
  width:100%;
  height:10%;
  margin-top:5%;
  display:flex;
  justify-content: space-between;
  flex-direction: row;
}
.icon{
    margin-left:5%;
    margin-top:1%;
    width:45rpx;  
    height:50rpx; 
}
.basic-icon{
  background: url("data:image/png;base64,");
  background-size: cover;
}
.anniversary-icon{
  background: url("data:image/jpg;base64,");
  background-size: cover;
}
.help-icon{
  background: url("data:image/png;base64,");
  background-size: cover;
}
.option-text{
  margin-left: 5%;
  width: 240rpx;
}
.arrow{
  width: 15rpx;
  height: 15rpx;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  transform: rotate(45deg);
  margin-left:20%;
  margin-right: 10%;
  margin-top:2%;
}
.separator{
  margin-top: 3%;
  height:5rpx;
  width:80%;
  margin-left:10%;
  background:rgb(222, 223, 219);
}
.bye{
  position:fixed;
  bottom:10rpx;
  left:40%;
  color:skyblue;
  font-size:30rpx;
}